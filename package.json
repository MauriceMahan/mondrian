    "postcss-loader": "^2.1.3",
    "sass-loader": "^6.0.7",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.1",
